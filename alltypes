  -- FROM	HOST-RESOURCES-MIB
  -- TEXTUAL CONVENTION AutonomousType
  SYNTAX	OBJECT IDENTIFIER
  MAX-ACCESS	read-only
  STATUS	current
  DESCRIPTION	"The type of storage represented by this entry."
