  DEFVAL	{ nonVolatile }
::= { iso(1) org(3) dod(6) internet(1) snmpV2(6) snmpModules(3) snmpVacmMIB(16) vacmMIBObjects(1) vacmMIBViews(5) vacmViewTreeFamilyTable(2) vacmViewTreeFamilyEntry(1) 5 }

