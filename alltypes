  -- FROM	RFC1213-MIB, IP-MIB
  SYNTAX	Counter32
  MAX-ACCESS	read-only
  STATUS	mandatory
  DESCRIPTION	"The number of IP datagrams that have been
            successfully fragmented at this entity."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) ip(4) 17 }

RFC1213-MIB::ipFragFails
ipFragFails OBJECT-TYPE
  -- FROM	RFC1213-MIB, IP-MIB
  SYNTAX	Counter32
  MAX-ACCESS	read-only
  STATUS	mandatory
  DESCRIPTION	"The number of IP datagrams that have been
            discarded because they needed to be fragmented at
            this entity but could not be, e.g., because their
            Don't Fragment flag was set."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) ip(4) 18 }

