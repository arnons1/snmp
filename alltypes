ifNumber OBJECT-TYPE
  -- FROM	RFC1213-MIB, IF-MIB
  SYNTAX	INTEGER
  MAX-ACCESS	read-only
  STATUS	mandatory
  DESCRIPTION	"The number of network interfaces (regardless of
            their current state) present on this system."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) interfaces(2) 1 }

RFC1213-MIB::ifTable
ifTable OBJECT-TYPE
  -- FROM	RFC1213-MIB, IF-MIB
  MAX-ACCESS	not-accessible
  STATUS	mandatory
  DESCRIPTION	"A list of interface entries.  The number of
            entries is given by the value of ifNumber."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) interfaces(2) 2 }

RFC1213-MIB::ifEntry
ifEntry OBJECT-TYPE
  -- FROM	RFC1213-MIB, IF-MIB
  MAX-ACCESS	not-accessible
  STATUS	mandatory
  INDEX		{ ifIndex }
  DESCRIPTION	"An interface entry containing objects at the
            subnetwork layer and below for a particular
            interface."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) interfaces(2) ifTable(2) 1 }

RFC1213-MIB::ifIndex
ifIndex OBJECT-TYPE
  -- FROM	RFC1213-MIB, IF-MIB
  SYNTAX	INTEGER
  MAX-ACCESS	read-only
  STATUS	mandatory
  DESCRIPTION	"A unique value for each interface.  Its value
            ranges between 1 and the value of ifNumber.  The
            value for each interface must remain constant at
            least from one re-initialization of the entity's
            network management system to the next re-
            initialization."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) interfaces(2) ifTable(2) ifEntry(1) 1 }

