  -- FROM	HOST-RESOURCES-MIB
  -- TEXTUAL CONVENTION ProductID
  SYNTAX	OBJECT IDENTIFIER
  MAX-ACCESS	read-only
  STATUS	current
  DESCRIPTION	"The product ID for this device."
