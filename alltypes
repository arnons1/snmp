NET-SNMP-EXTEND-MIB::nsExtendInput
nsExtendInput OBJECT-TYPE
  -- FROM	NET-SNMP-EXTEND-MIB
  -- TEXTUAL CONVENTION DisplayString
  SYNTAX	OCTET STRING (0..255) 
  DISPLAY-HINT	"255a"
  MAX-ACCESS	read-create
  STATUS	current
  DESCRIPTION	"The standard input for the command"
  DEFVAL	{ 0 }
::= { iso(1) org(3) dod(6) internet(1) private(4) enterprises(1) netSnmp(8072) netSnmpObjects(1) nsExtensions(3) nsExtendObjects(2) nsExtendConfigTable(2) nsExtendConfigEntry(1) 4 }

NET-SNMP-EXTEND-MIB::nsExtendCacheTime
nsExtendCacheTime OBJECT-TYPE
  -- FROM	NET-SNMP-EXTEND-MIB
  SYNTAX	INTEGER
  MAX-ACCESS	read-create
  STATUS	current
  DESCRIPTION	"The length of time for which the output of
       this command will be cached.  During this time,
       retrieving the output-related values will not
       reinvoke the command.
       A value of -1 indicates that the output results
       should not be cached at all, and retrieving each
       individual output-related value will invoke the
       command afresh."
  DEFVAL	{ 5 }
::= { iso(1) org(3) dod(6) internet(1) private(4) enterprises(1) netSnmp(8072) netSnmpObjects(1) nsExtensions(3) nsExtendObjects(2) nsExtendConfigTable(2) nsExtendConfigEntry(1) 5 }

NET-SNMP-EXTEND-MIB::nsExtendExecType
nsExtendExecType OBJECT-TYPE
  -- FROM	NET-SNMP-EXTEND-MIB
  SYNTAX	INTEGER {exec(1), shell(2)} 
  MAX-ACCESS	read-create
  STATUS	current
  DESCRIPTION	"The mechanism used to invoke the command."
  DEFVAL	{ exec }
::= { iso(1) org(3) dod(6) internet(1) private(4) enterprises(1) netSnmp(8072) netSnmpObjects(1) nsExtensions(3) nsExtendObjects(2) nsExtendConfigTable(2) nsExtendConfigEntry(1) 6 }

NET-SNMP-EXTEND-MIB::nsExtendRunType
nsExtendRunType OBJECT-TYPE
  -- FROM	NET-SNMP-EXTEND-MIB
  SYNTAX	INTEGER {run-on-read(1), run-on-set(2), run-command(3)} 
  MAX-ACCESS	read-create
  STATUS	current
  DESCRIPTION	"Used to implement 'push-button' command invocation.
       The command for a 'run-on-read' entry will be invoked
       whenever one of the corresponding output-related
       instances is requested (and assuming the cached value
       is not still current).
       The command for a 'run-on-set' entry will only be invoked
       on receipt of a SET assignment for this object with the
       value 'run-command'.
       Reading an instance of this object will always return either
       'run-on-read' or 'run-on-set'.
      "
  DEFVAL	{ run-on-read }
::= { iso(1) org(3) dod(6) internet(1) private(4) enterprises(1) netSnmp(8072) netSnmpObjects(1) nsExtensions(3) nsExtendObjects(2) nsExtendConfigTable(2) nsExtendConfigEntry(1) 7 }

