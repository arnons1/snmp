  MAX-ACCESS	read-create
  STATUS	current
  DESCRIPTION	"The storage type for this conceptual row."
  DEFVAL	{ volatile }
::= { iso(1) org(3) dod(6) internet(1) private(4) enterprises(1) netSnmp(8072) netSnmpObjects(1) nsExtensions(3) nsExtendObjects(2) nsExtendConfigTable(2) nsExtendConfigEntry(1) 20 }

