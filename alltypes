  -- FROM	SCTP-MIB
  -- TEXTUAL CONVENTION TimeStamp
  SYNTAX	TimeTicks
  MAX-ACCESS	read-only
  STATUS	current
  DESCRIPTION	"The value of sysUpTime at the time that this row was
       created."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) sctpMIB(104) sctpObjects(1) sctpAssocRemAddrTable(5) sctpAssocRemAddrEntry(1) 8 }

