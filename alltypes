  -- FROM	AGENTX-MIB
  -- TEXTUAL CONVENTION TimeStamp
  SYNTAX	TimeTicks
  MAX-ACCESS	read-only
  STATUS	current
  DESCRIPTION	"The value of sysUpTime when the last row creation or deletion
      occurred in the agentxSessionTable.
     "
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) agentxMIB(74) agentxObjects(1) agentxSession(3) 1 }

