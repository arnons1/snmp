  -- FROM	DISMAN-SCHEDULE-MIB
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) schedMIB(63) 1 }

DISMAN-SCHEDULE-MIB::schedLocalTime
schedLocalTime OBJECT-TYPE
  -- FROM	DISMAN-SCHEDULE-MIB
  -- TEXTUAL CONVENTION DateAndTime
  SYNTAX	OCTET STRING (11) 
  DISPLAY-HINT	"2d-1d-1d,1d:1d:1d.1d,1a1d:1d"
  MAX-ACCESS	read-only
  STATUS	current
  DESCRIPTION	"The local time used by the scheduler.  Schedules which
         refer to calendar time will use the local time indicated
         by this object.  An implementation MUST return all 11 bytes
         of the DateAndTime textual-convention so that a manager
         may retrieve the offset from GMT time."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) schedMIB(63) schedObjects(1) 1 }

DISMAN-SCHEDULE-MIB::schedTable
schedTable OBJECT-TYPE
  -- FROM	DISMAN-SCHEDULE-MIB
  MAX-ACCESS	not-accessible
  STATUS	current
  DESCRIPTION	"This table defines scheduled actions triggered by
         SNMP set operations."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) schedMIB(63) schedObjects(1) 2 }

DISMAN-SCHEDULE-MIB::schedEntry
schedEntry OBJECT-TYPE
  -- FROM	DISMAN-SCHEDULE-MIB
  MAX-ACCESS	not-accessible
  STATUS	current
  INDEX		{ schedOwner, schedName }
  DESCRIPTION	"An entry describing a particular scheduled action.

         Unless noted otherwise, writable objects of this row
         can be modified independent of the current value of
         schedRowStatus, schedAdminStatus and schedOperStatus.
         In particular, it is legal to modify schedInterval
         and the objects in the schedCalendarGroup when
         schedRowStatus is active and schedAdminStatus and
         schedOperStatus are both enabled."
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) schedMIB(63) schedObjects(1) schedTable(2) 1 }

DISMAN-SCHEDULE-MIB::schedOwner
