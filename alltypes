  -- FROM	IP-MIB
  SYNTAX	Unsigned32 (1500..65535) 
  UNITS		"octets"
  MAX-ACCESS	read-only
  STATUS	current
  DESCRIPTION	"The size of the largest IPv6 datagram that this entity can
