  -- FROM	IP-MIB
  SYNTAX	Unsigned32
  UNITS		"milliseconds"
  MAX-ACCESS	read-only
  STATUS	current
  DESCRIPTION	"The time a neighbor is considered reachable after receiving
            a reachability confirmation."
