::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) notificationLogMIB(92) notificationLogMIBObjects(1) nlmLog(3) nlmLogVariableTable(2) nlmLogVariableEntry(1) 10 }

