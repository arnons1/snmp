  -- FROM	BRIDGE-MIB
  -- TEXTUAL CONVENTION MacAddress
  SYNTAX	OCTET STRING (6) 
  DISPLAY-HINT	"1x:"
  MAX-ACCESS	read-only
  STATUS	current
