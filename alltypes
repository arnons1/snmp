  -- FROM	DISMAN-SCRIPT-MIB
  -- TEXTUAL CONVENTION DateAndTime
  SYNTAX	OCTET STRING (8 | 11) 
  DISPLAY-HINT	"2d-1d-1d,1d:1d:1d.1d,1a1d:1d"
  MAX-ACCESS	read-only
  STATUS	current
  DESCRIPTION	"The date and time when the smRunResult was last updated.
         The value '0000000000000000'H is returned if smRunResult
         has not yet been updated after the creation of this
         smRunTable entry."
  DEFVAL	{ 0 }
::= { iso(1) org(3) dod(6) internet(1) mgmt(2) mib-2(1) scriptMIB(64) smObjects(1) smRunObjects(4) smRunTable(2) smRunEntry(1) 12 }

